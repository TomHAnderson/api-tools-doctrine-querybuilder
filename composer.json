{
    "name": "laminas-api-tools/api-tools-doctrine-querybuilder",
    "description": "Laminas API Tools Doctrine QueryBuilder module",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "api-tools",
        "api",
        "doctrine",
        "filter"
    ],
    "homepage": "https://api-tools.getlaminas.org",
    "support": {
        "docs": "https://api-tools.getlaminas.org/documentation",
        "issues": "https://github.com/laminas-api-tools/api-tools-doctrine-querybuilder/issues",
        "source": "https://github.com/laminas-api-tools/api-tools-doctrine-querybuilder",
        "rss": "https://github.com/laminas-api-tools/api-tools-doctrine-querybuilder/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laminas": {
            "module": [
                "Laminas\\ApiTools\\Doctrine\\ODM\\QueryBuilder"
            ]
        }
    },
    "require": {
        "php": "^7.2",
        "doctrine/doctrine-module": "^3.0",
        "doctrine/doctrine-mongo-odm-module": "dev-develop",
        "laminas/laminas-hydrator": "^3.0",
        "laminas/laminas-modulemanager": "^2.7.2",
        "laminas/laminas-mvc": "^3.0.2",
        "laminas/laminas-servicemanager": "^3.1.1",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "laminas-api-tools/api-tools-provider": "^1.2",
        "laminas/laminas-coding-standard": "~1.0.0",
        "laminas/laminas-i18n": "^2.7.3",
        "laminas/laminas-log": "^2.9.1",
        "laminas/laminas-router": "^3.3",
        "laminas/laminas-serializer": "^2.8",
        "laminas/laminas-test": "3.0.1",
        "php-coveralls/php-coveralls": "^2.2",
        "phpunit/phpunit": "^5.0",
        "symfony/yaml": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\ApiTools\\Doctrine\\ODM\\QueryBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\ApiTools\\Doctrine\\ODM\\QueryBuilder\\": "test/"
        }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover build/logs/clover.xml"
    }
}
